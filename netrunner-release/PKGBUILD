pkgname=netrunner-release
pkgver=$(date +%Y.%m)
pkgrel=1
pkgdesc="Adds a Netrunner Rolling lsb-release file"
arch=('any')
url="N/A"
license=('GPL')
groups=('netrunner-common')
depends=('bash')
source=("lsb-release")
md5sums=('SKIP')

package() {
  install -Dm755 lsb-release $pkgdir/etc/lsb-release
}
