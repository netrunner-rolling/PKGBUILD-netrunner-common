pkgname=netrunner-settings-samba
pkgver=$(date +%Y%m%d)
pkgrel=1
pkgdesc="Adds a smb.conf file for Netrunner Rolling"
arch=('any')
url="N/A"
license=('GPL')
groups=('netrunner-common')
depends=('bash')
source=("smb.conf")
md5sums=('SKIP')

package() {
  install -Dm755 smb.conf $pkgdir/etc/samba/smb.conf
}
